    <div class="container">
      <div class="row row-cols-2 justify-content-md-center mt-5">

        <div class="col col-3">
          <!-- 
           -->

          <div class="row">
            <legend class="text-primary"><b>Search Filters</b></legend>
            <div class="accordion" id="accordionExample">
              <div class="accordion-item">
                <div class="list-group">
                  <a href="#" class="list-group-item list-group-item-action">By
                    My Location</a>
                </div>

              </div>
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingTwo">
                  <button class="accordion-button collapsed" type="button"
                    data-bs-toggle="collapse" data-bs-target="#collapseTwo"
                    aria-expanded="false" aria-controls="collapseTwo">
                    Exchange Value
                  </button>
                </h2>
                <div id="collapseTwo" class="accordion-collapse collapse"
                  aria-labelledby="headingTwo"
                  data-bs-parent="#accordionExample">
                  <div class="form-group row mx-auto mt-2">

                    <label for="exValue" class="col-sm-2 col-form-label">Value</label>
                    <div class="col-sm-10">
                      <input type="number" style="max-width:30%"
                        class="form-control form-control-sm" id="exchangeValue"
                        min="1" max="5">

                    </div>

                  </div>
                </div>
              </div>
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingThree">
                  <button class="accordion-button" type="button"
                    data-bs-toggle="collapse" data-bs-target="#collapseThree"
                    aria-expanded="true" aria-controls="collapseThree">
                    Intent Type
                  </button>
                </h2>
                <div id="collapseThree" class="accordion-collapse collapse show"
                  aria-labelledby="headingThree"
                  data-bs-parent="#accordionExample">
                  <div class="accordion-body">
                    <div class="form-check">
                      <label class="form-check-label">
                        <input type="radio" class="form-check-input"
                          name="optionsRadios" id="optionsRadios1"
                          value="option1" checked="">
                        Donation
                      </label>
                    </div>
                    <div class="form-check">
                      <label class="form-check-label">
                        <input type="radio" class="form-check-input"
                          name="optionsRadios" id="optionsRadios2"
                          value="option2">
                        Exchange
                      </label>
                    </div>
                  </div>
                </div>
              </div>
              <div class="form-floating mt-2 pt-2">
                <button class="btn btn-primary px-5" type="submit">Apply</button>
              </div>
            </div>
          </div>

